{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "server": "nodemon app.js",
    "build-o": "cd app && npm run build",
    "install-client": "cd app && npm install",
    "client": "npm start --prefix app",
    "concurrent": "concurrently \"npm run dev\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run install-client && npm run build-o"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.7-alpha.1",
    "formidable": "^1.2.2",
    "fs": "0.0.1-security",
    "gridfs-stream": "^1.1.1",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "ip": "^1.1.5",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.11.8",
    "mongoose-type-email": "^1.0.12",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "node-mailjet": "^3.3.1",
    "nodemailer": "^6.4.16",
    "path": "^0.12.7",
    "serialport": "^9.0.0",
    "socket.io": "^2.3.0",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
